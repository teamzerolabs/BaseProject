    "@nestjs/typeorm": "6.1.3",
    "@slack/web-api": "^5.1.0",
    "backoff-rxjs": "^6.3.2",
    "bcrypt": "^3.0.6",
    "chance": "^1.0.18",
    "class-transformer": "^0.2.3",
